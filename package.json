{
  "name": "botInterface",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build && blitz db seed",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "start:production": "blitz start --port $PORT"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node db/seeds.ts"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "1.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@prisma/client": "3.11.1",
    "blitz": "0.45.3",
    "formik": "2.2.9",
    "framer-motion": "5.x",
    "react": "18.0.0-beta-149b420f6-20211119",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "zod": "3.14.3"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.43",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.6.1",
    "prettier-plugin-prisma": "3.11.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "prisma": "3.11.1",
    "typescript": "~4.5"
  },
  "private": true
}
